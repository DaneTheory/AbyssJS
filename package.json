{
  "name": "abyss-starter",
  "version": "1.0.0",
  "description": "Boilerplate for starting app dev. Includes setup for React/Redux, ES6, and other goodies",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "dev": "npm-run-all --parallel open:src",
    "start": "gulp",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist"
  },
  "author": "Branden Dane",
  "license": "MIT",
  "dependencies": {
    "object-assign": "4.1.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.12.10",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "connect-logger": "0.0.1",
    "coveralls": "2.11.9",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-cache": "0.4.5",
    "gulp-changed": "1.3.0",
    "gulp-cssnano": "2.1.2",
    "gulp-imagemin": "3.0.1",
    "gulp-notify": "2.2.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.3.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "node-notifier": "4.6.0",
    "node-sass": "3.7.0",
    "npm-run-all": "2.1.1",
    "redux-immutable-state-invariant": "1.2.3",
    "require-dir": "0.3.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danetheory/abyss-starter"
  }
}
